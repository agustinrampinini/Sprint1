    <section id="main"
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">
Residences List</h3>
			</div>
			<div class="panel-body">
				<table class="table table-striped table-hover">
				<tr>
					<th>Nombre</th>
					<th>Ubicacion</th>
					<th>Disponibilidad</th>
				</tr>		
				<% @homes.each do |home| %>
					<tr>
						<td><%= home.nombre%></td>
						<td>
						<%= home.ubicacion%>
						</td>
						<td><%= home.disponibilidad%></td>
						<td><%= link_to "Edit",edit_home_path(home),class: "btn btn-default"%>
						<%= link_to "Delet",home,method: :delete,data:{confirm: "Estas seguro?"},class: "btn btn-default"%>
						</td>
					</tr>
				<%end%>		
				</table>
			</div>
		</div>
    </section>
